jjiijjiijjiijjiijjiijjiijjiijjiijjiijjiijjiijjiijjiijjiijjiijjiijjiijjiijjiijjiiwhile wait() do 
game:GetService("ReplicatedStorage").packages.Net:FindFirstChild("RE/DailyReward/Claim"):FireServer()
endlocal player = game:GetService("Players").LocalPlayer
local replicatedStorage = game:GetService("ReplicatedStorage")
local event = replicatedStorage:WaitForChild("Events"):WaitForChild("Boxing")

local gui = Instance.new("ScreenGui")
gui.Parent = player:WaitForChild("PlayerGui")

local frame = Instance.new("Frame")
frame.Parent = gui
frame.Size = UDim2.new(0, 180, 0, 130)
frame.Position = UDim2.new(0.5, -90, 0.5, -65)
frame.BackgroundColor3 = Color3.fromRGB(math.random(100, 255), math.random(100, 255), math.random(100, 255))
frame.BorderSizePixel = 3
frame.Active = true
frame.Draggable = true

local toggleButton = Instance.new("TextButton")
toggleButton.Parent = frame
toggleButton.Size = UDim2.new(1, -10, 0, 50)
toggleButton.Position = UDim2.new(0, 5, 0, 5)
toggleButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- start red
toggleButton.TextColor3 = Color3.new(1, 1, 1)
toggleButton.Text = "Auto Farm: Off"
toggleButton.Font = Enum.Font.SourceSansBold
toggleButton.TextSize = 20
toggleButton.BorderSizePixel = 3

local boxerButton = Instance.new("TextButton")
boxerButton.Parent = frame
boxerButton.Size = UDim2.new(1, -10, 0, 50)
boxerButton.Position = UDim2.new(0, 5, 0, 60)
boxerButton.BackgroundColor3 = Color3.fromRGB(math.random(100, 255), math.random(100, 255), math.random(100, 255))
boxerButton.TextColor3 = Color3.new(1, 1, 1)
boxerButton.Text = "Get Boxer"
boxerButton.Font = Enum.Font.SourceSansBold
boxerButton.TextSize = 20
boxerButton.BorderSizePixel = 3

local credits = Instance.new("TextLabel")
credits.Parent = boxerButton
credits.Size = UDim2.new(1, 0, 0, 15)
credits.Position = UDim2.new(0, 0, 1, 0)
credits.BackgroundTransparency = 1
credits.Text = "Credits: Incognito Scripts"
credits.TextColor3 = Color3.new(1, 1, 1)
credits.Font = Enum.Font.SourceSansBold
credits.TextSize = 14

local looping = false
local connection

toggleButton.MouseButton1Click:Connect(function()
    looping = not looping

    if looping then
        toggleButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0) -- green when on
        toggleButton.Text = "Auto Farm: On"
        connection = game:GetService("RunService").RenderStepped:Connect(function()
            event:FireServer(player, false)
        end)
    else
        toggleButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- red when off
        toggleButton.Text = "Auto Farm: Off"
        if connection then
            connection:Disconnect()
            connection = nil
        end
    end
end)

boxerButton.MouseButton1Click:Connect(function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/Pro666Pro/autogetboxerglove/main/main.lua'))()
end)jjiijjiilocal player = game:GetService("Players").LocalPlayer
